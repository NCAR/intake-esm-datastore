{
  "esmcat_version": "0.1.0",
  "id": "glade-cmip5",
  "description": "This is an ESM collection for CMIP5 data accessible on the NCAR's GLADE disk storage system in /glade/collections/cmip/cmip5",
  "catalog_file": "/glade/collections/cmip/catalog/intake-esm-datastore/catalogs/glade-cmip5.csv.gz",
  "attributes": [
    {
      "column_name": "product_id",
      "vocabulary": ""
    },
    {
      "column_name": "institute",
      "vocabulary": ""
    },
    {
      "column_name": "model",
      "vocabulary": ""
    },
    {
      "column_name": "experiment",
      "vocabulary": ""
    },
    { "column_name": "ensemble_member", "vocabulary": "" },
    {
      "column_name": "frequency",
      "vocabulary": ""
    },
    { "column_name": "variable", "vocabulary": "" },
    {
      "column_name": "modeling_realm",
      "vocabulary": ""
    },
    {
      "column_name": "version",
      "vocabulary": ""
    },
    {
      "column_name": "mip_table",
      "vocabulary": ""
    },
      {
      "column_name": "temporal_subset",
      "vocabulary": ""
    }
  ],
  "assets": {
    "column_name": "path",
    "format": "netcdf"
  },
  "aggregation_control": {
    "variable_column_name": "variable",
    "groupby_attrs": [
      "product_id",
      "institute",
      "model",
      "modeling_realm",
      "experiment",
      "frequency",
      "mip_table"
    ],
    "aggregations": [
      {
        "type": "union",
        "attribute_name": "variable"
      },
      {
        "type": "join_existing",
        "attribute_name": "temporal_subset",
        "options": { "dim": "time", "coords": "minimal", "compat": "override" }
      },
      {
        "type": "join_new",
        "attribute_name": "ensemble_member",
        "options": { "coords": "minimal", "compat": "override" }
      }
    ]
  }
}
